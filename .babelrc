{
  "presets": [
    ["@babel/env", { "targets": { "node": "current" }, "modules": false }],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
    "presets": ["minify"]
    },
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  }
}
